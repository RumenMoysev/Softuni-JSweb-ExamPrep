<section id="viewCatalog" class="background-img">
    <!--If there are one or more electronics-->
    {{#if electronics}}
    <div class="added-electronics">
        {{#each electronics}}
        <a href="#" class="added-electronics-in-market">
            <img src="./image/laptop.webp" class="picture-added-electronics">
            <h3>Laptop HP EliteBook 820 G4</h3>
            <span>Price: $153</span>
        </a>
        {{/each}}
    </div>

    <!--If there are no electronics in catalog yet-->
    {{else}}
    <div class="guest">
        There are no electronics found...
    </div>
    {{/if}}
</section>